class Solution {
public:
    int search(vector<int>& nums, int target) {
        
        int index =-2;
        int size = nums.size();
        for(int i =0; i< size;i++)
        {
            if(nums[i] == target)
                index = i;
        }
        if (index!= -2){
            return index;}
        else 
        {return -1;}
    }
};
